{
    "env": {
      "browser": true,
      "es6": true,
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": ["react"],
    "parser":"babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
    },
    "rules": {
      "no-unused-vars": "warn",
      "react/react-in-jsx-scope": "off",
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
}